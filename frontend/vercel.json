{
  "version": 2,
  "name": "fra-atlas",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "REACT_APP_API_URL": "https://fra-atlas-backend-ipd3.onrender.com/api",
    "REACT_APP_DATA_PROCESSOR_URL": "https://fra-atlas-data-processor.onrender.com",
    "REACT_APP_DIGITIZATION_PIPELINE_URL": "https://fra-atlas-digitization-pipeline.onrender.com"
  },
  "build": {
    "env": {
      "GENERATE_SOURCEMAP": "false",
      "CI": "false"
    }
  }
}